        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="icon" type="image/x-icon" href="{{ asset('build/images/bookstore.ico') }}" />
        <title>{% block title %}BookStore{% endblock %}</title>
        {% block stylesheets %}
